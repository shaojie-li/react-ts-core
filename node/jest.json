{
  "verbose": true,
  "rootDir": "..",
  "roots": ["<rootDir>/test"],
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "\\.test\\.tsx?$",
  "testURL": "http://127.0.0.1",
  "moduleFileExtensions": ["ts", "tsx", "js"],
  "moduleDirectories": ["node_modules", "src"],
  "globals": {
    "ts-jest": {
      "tsConfig": "./node/tsconfig.json"
    }
  }
}
